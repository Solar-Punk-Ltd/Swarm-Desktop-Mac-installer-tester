
	delete_install := exec.Command("rm", "-r", "/Applications/Swarm Desktop.app")
	delete_out, delete_err := delete_install.Output()

	if delete_err != nil {
		fmt.Println("Delete error:", delete_err)
		log.Println("Delete error", string(delete_err.Error()))
	} else {
		fmt.Println("Delete success:", string(delete_out))
		log.Println("Delete success:", string(delete_out))
	}

	time.Sleep(3 * time.Second)
	delete_source := exec.Command("rm", "-r", "Swarm.Desktop-0.30.0-arm64.dmg")
	delete_source_out, delete_source_err := delete_source.Output()

	if delete_source_err != nil {
		fmt.Println("Delete source error:", delete_source_err)
		log.Println("Delete source error:", string(delete_source_err.Error()))
	} else {
		fmt.Println("Delete source success:", string(delete_source_out))
		log.Println("Delete source success:", string(delete_source_out))
	}

